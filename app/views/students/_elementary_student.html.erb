<tr class="student">
    <td><%= link_to "編集", edit_student_path(elementary_student.id), class: "edit_result_path"%></td>
    <td class="table_item"><%= elementary_student.id %></td>
    <td class="table_item"><%= elementary_student.grade %>年</td>
    <td class="table_item"><%= elementary_student.school.school %></td>
    <td class="table_item"><%= elementary_student.year %></td>
    <td class="table_item"><a href="/students/<%= elementary_student.id %>" class="result_student_link"><%= elementary_student.family_name %></a></td>
    <td class="table_item"><a href="/students/<%= elementary_student.id %>" class="result_student_link"><%= elementary_student.given_name %></a></td>
    <td class="table_item"><%= elementary_student.family_name_kana %></td>
    <td class="table_item"><%= elementary_student.given_name_kana %></td>
    <td class="table_item"><%= elementary_student.student_type.student_type %></td>
</tr>