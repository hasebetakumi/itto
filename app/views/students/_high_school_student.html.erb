    <td class="table_item"><%= high_school_student.grade %></td>
    <td class="table_item"><%= high_school_student.school.school %></td>
    <td class="table_item"><%= high_school_student.year %></td>
    <td class="table_item"><a href="/students/<%= high_school_student.id %>" class="result_student_link"><%= high_school_student.family_name %></a></td>
    <td class="table_item"><a href="/students/<%= high_school_student.id %>" class="result_student_link"><%= high_school_student.given_name %></a></td>
    <td class="table_item"><%= high_school_student.family_name_kana %></td>
    <td class="table_item"><%= high_school_student.given_name_kana %></td>
    <td class="table_item"><%= high_school_student.student_type.student_type %></td>
